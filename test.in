[
    [let [[n int][a real][b real][area real]]]
    
    [let [[width real][x real]]]

    [:= n 100]
    [:= a 0] [:= b 1] [:= area 0]
    [:= x a]
    [:= width [/ [- b a] n]]
   
    [while [< x b]
        [:= area 
            [+ area [* width [* x x]]]
        ]

        [:= x
            [+ x width]
        ]
    ]

    [stdout area]
]

