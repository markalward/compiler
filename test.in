[
    [let [[a b c][int int int]]
       []  
    ]

    [let [[n int][a float][b float][area float]]]
    
    [let [[width float][x float]]]

    [:= n 100]
    [:= a 0] [:= b 1] [:= area 0]
    [:= x a]
    [:= width [/ [- b a] n]]
   
    [while [< x b]
        [:= area 
            [+ area [* width [* x x]]]
        ]

        [:= x
            [+ x width]
        ]
    ]

    [stdout area]
]

